<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>servletlogin</servlet-name>
        <servlet-class>controller.ServletLogin</servlet-class>
        <init-param>
            <param-name>user</param-name>
            <param-value>root</param-value>
        </init-param>
        <init-param>
            <param-name>pass</param-name>
            <param-value>root</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>servletadmin</servlet-name>
        <servlet-class>controller.ServletAdmin</servlet-class>
        <init-param>
            <param-name>admin</param-name>
            <param-value>admin</param-value>
        </init-param>
        <init-param>
            <param-name>word</param-name>
            <param-value>pass</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>servletsave</servlet-name>
        <servlet-class>controller.ServletSave</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletdelete</servlet-name>
        <servlet-class>controller.ServletDelete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletedit</servlet-name>
        <servlet-class>controller.ServletEdit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletupdate</servlet-name>
        <servlet-class>controller.ServletUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletview</servlet-name>
        <servlet-class>controller.ServletView</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletcand</servlet-name>
        <servlet-class>controller.ServletCand</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletvot</servlet-name>
        <servlet-class>controller.ServletVot</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletcast</servlet-name>
        <servlet-class>controller.ServletCast</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletwin</servlet-name>
        <servlet-class>controller.ServletWin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletlogin</servlet-name>
        <url-pattern>/servletlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletadmin</servlet-name>
        <url-pattern>/servletadmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletsave</servlet-name>
        <url-pattern>/servletsave</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletdelete</servlet-name>
        <url-pattern>/servletdelete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletedit</servlet-name>
        <url-pattern>/servletedit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletupdate</servlet-name>
        <url-pattern>/servletupdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletview</servlet-name>
        <url-pattern>/servletview</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletcand</servlet-name>
        <url-pattern>/servletcand</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletvot</servlet-name>
        <url-pattern>/servletvot</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletcast</servlet-name>
        <url-pattern>/servletcast</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletwin</servlet-name>
        <url-pattern>/servletwin</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
